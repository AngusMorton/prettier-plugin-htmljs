export interface Logo {
  imgUrl: string;
  imgAlt: string;
}

<macro|{ imgUrl, imgAlt }: Logo| name="logo-image">
  <li>
    <img class="h-7 lg:h-9" src=imgUrl alt=imgAlt title=imgAlt loading="lazy" />
  </li>
</macro>

<macro|{ imgUrl, imgAlt }: Logo, anotherOne: Logo| name="logo-image">
  <li>
    <img class="h-7 lg:h-9" src=imgUrl alt=imgAlt title=imgAlt loading="lazy" />
  </li>
</macro>